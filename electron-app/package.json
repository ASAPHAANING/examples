{
  "name": "electron-app",
  "version": "0.1.0",
  "description": "A simple Electron app using a Neon package",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-build-env neon build neon-hello --release",
    "install": "npm run build"
  },
  "devDependencies": {
    "electron-build-env": "^0.2",
    "neon-cli": "^0.2.0"
  },
  "dependencies": {
    "electron": "3.0.8",
    "neon-hello": "^0.1.1"
  },
  "eslintConfig": {
    "rules": {
      "import/no-unresolved": "off"
    }
  }
}
